{"components/common/footer":"{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}}\n<footer class=\"footer\" role=\"contentinfo\">\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\n    <div class=\"container\">\n        <section class=\"footer-info\">\n            <article class=\"footer-info-col store-info\" data-section-type=\"footer-webPages\">\n                <h3 class=\"footer-info-heading logo-heading\">\n                    <img class=\"lazyload\" src=\"https://cdn11.bigcommerce.com/s-ngh35wij02/images/stencil/original/image-manager/{{theme_settings.footer_image}}\" alt=\"GatCrank\"/>\n                    {{theme_settings.footer_store_title}}\n                </h3>\n                    <p>\n                        {{theme_settings.footer_address}}\n                    </p>\n                    <a class=\"footer-email\" href=\"{{theme_settings.footer_email}}\">{{theme_settings.footer_email_text}}</a>\n            </article>\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h3 class=\"footer-info-heading\">Quicklinks</h3>\n                <ul class=\"footer-info-list\">\n                   <li>\n                        <a href=\"{{theme_settings.footer_link1}}\">{{theme_settings.footer_linktext1}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link2}}\">{{theme_settings.footer_linktext2}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link3}}\">{{theme_settings.footer_linktext3}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link4}}\">{{theme_settings.footer_linktext4}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link5}}\">{{theme_settings.footer_linktext5}}</a>\n                    </li> \n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\n                <h3 class=\"footer-info-heading\">Info</h3>\n                <ul class=\"footer-info-list\">\n                    <li>\n                        <a href=\"{{theme_settings.footer_link6}}\">{{theme_settings.footer_linktext6}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link7}}\">{{theme_settings.footer_linktext7}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link8}}\">{{theme_settings.footer_linktext8}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link9}}\">{{theme_settings.footer_linktext9}}</a>\n                    </li>\n                    <li>\n                        <a href=\"{{theme_settings.footer_link10}}\">{{theme_settings.footer_linktext10}}</a>\n                    </li>\n                   \n                </ul>\n            </article>\n\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h3>\n                <ul class=\"footer-info-list\">\n                    {{#each shop_by_brand}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                </ul>\n            </article>\n            {{/and}}\n            {{#if theme_settings.show_subscription_form}}\n                <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\n                    {{#if settings.show_newsletter_box}}\n                        {{> components/common/subscription-form}}\n                    {{/if}}\n                    {{> components/common/payment-icons}}\n                </article>\n            {{/if}}\n            {{#if theme_settings.show_subscription_form}}\n                <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\n                    {{#if settings.show_newsletter_box}}\n                        {{> components/common/subscription-form}}\n                    {{/if}}\n                    {{> components/common/payment-icons}}\n                </article>\n            {{/if}}\n            {{#if theme_settings.show_social_media}}\n                <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\n                    <h3 class=\"footer-info-heading\">{{lang 'social.connect'}}</h3>\n                    {{> components/common/social-links}}\n                </article>\n            {{/if}}\n        </section>\n        {{#if settings.paypal_commerce_credit_message}}\n        <div class=\"footer-copyright\">\n            <p class=\"paypal-credit\">{{nl2br settings.paypal_commerce_credit_message}}</p>\n        </div>\n        {{/if}}\n        {{#if theme_settings.show_powered_by}}\n            <div class=\"footer-copyright\">\n                <p class=\"powered-by\">{{lang 'footer.powered_by'}} <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\n            </div>\n        {{/if}}\n        {{#if theme_settings.show_copyright_footer}}\n            <div class=\"footer-copyright\">\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}, All rights reserved </p>\n            </div>\n        {{/if}}\n    </div>\n    {{{region name=\"ssl_site_seal--global\"}}}\n</footer>\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_klarna}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-klarna\"></use></svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                    {{#if display_name '===' 'facebook'}}\n                        <svg width=\"40\" height=\"40\" viewBox=\"0 0 40 40\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                            <path d=\"M20 0C8.955 0 0 9.00113 0 20.103C0 30.1813 7.38667 38.504 17.01 39.9581V25.4303H12.0617V20.1466H17.01V16.6302C17.01 10.8087 19.8317 8.25397 24.645 8.25397C26.95 8.25397 28.17 8.42652 28.7467 8.50358V13.1156H25.4633C23.42 13.1156 22.7067 15.0639 22.7067 17.2584V20.1466H28.695L27.8833 25.4303H22.7067V40C32.4683 38.6698 40 30.2802 40 20.103C40 9.00113 31.045 0 20 0Z\" fill=\"white\"/>\n                        </svg>\n                        {{else if display_name '===' 'instagram'}}\n                        <svg width=\"40\" height=\"40\" viewBox=\"0 0 40 40\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n                            <path d=\"M11.8182 0C5.2983 0 0 5.2983 0 11.8182V28.1818C0 34.7017 5.2983 40 11.8182 40H28.1818C34.7017 40 40 34.7017 40 28.1818V11.8182C40 5.2983 34.7017 0 28.1818 0H11.8182ZM30.9091 7.27273C31.9105 7.27273 32.7273 8.08949 32.7273 9.09091C32.7273 10.0923 31.9105 10.9091 30.9091 10.9091C29.9077 10.9091 29.0909 10.0923 29.0909 9.09091C29.0909 8.08949 29.9077 7.27273 30.9091 7.27273ZM20 10C25.5185 10 30 14.4815 30 20C30 25.5185 25.5185 30 20 30C14.4815 30 10 25.5185 10 20C10 14.4815 14.4815 10 20 10ZM20 11.8182C15.4901 11.8182 11.8182 15.4901 11.8182 20C11.8182 24.5099 15.4901 28.1818 20 28.1818C24.5099 28.1818 28.1818 24.5099 28.1818 20C28.1818 15.4901 24.5099 11.8182 20 11.8182Z\" fill=\"white\"/>\n                        </svg>\n                        {{else}}\n                        <svg>\n                            <use xlink:href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                       id=\"nl_email\"\n                       name=\"nl_email\"\n                       type=\"email\"\n                       value=\"{{customer.email}}\"\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                       aria-describedby=\"alertBox-message-text\"\n                       aria-required=\"true\"\n                       required\n                >\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\n                       type=\"submit\"\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\n                >\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n"}
